{
  "expo": {
    "name": "SpeakFlow",
    "slug": "speakflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ciui9494.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.ciui9494.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "SpeakFlow - English Practice",
      "shortName": "SpeakFlow",
      "description": "Practice English with YouTube videos",
      "display": "standalone",
      "themeColor": "#4285f4",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-audio"
    ],
    "permissions": [
      "RECORD_AUDIO",
      "MODIFY_AUDIO_SETTINGS"
    ],
    "extra": {
      "eas": {
        "projectId": "68535394-058b-4e52-a2df-2050cd2109e7"
      }
    },
    "owner": "ciui9494"
  }
}
